{
  "name": "exercise-log",
  "version": "0.0.0",
  "engines": {
    "node": ">=16.10.0"
  },
  "scripts": {
    "ng": "node_modules/.bin/ng",
    "start": "node_modules/.bin/ng serve --disable-host-check",
    "build": "node --max-old-space-size=1024 node_modules/.bin/ng build",
    "watch": "node_modules/.bin/ng build --watch --configuration development",
    "test": "node_modules/.bin/ng test",
    "test:ci": "node_modules/.bin/ng test --no-watch --code-coverage --no-progress --browsers=ChromeHeadlessCI",
    "lint": "node_modules/.bin/ng lint",
    "lint:fix": "node_modules/.bin/ng lint --fix",
    "lint:styles": "node_modules/.bin/stylelint ./src/**/*.scss",
    "lint:styles:fix": "node_modules/.bin/stylelint ./src/**/*.scss --fix",
    "lint:ci": "npm run lint && npm run lint:styles && npm run format:check",
    "format:check": "node_modules/.bin/prettier --check .",
    "format:write": "node_modules/.bin/prettier --write .",
    "prepare": "husky install",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "ci:start-server": "angular-http-server --path ./dist/mvp-enterprise -p 4200",
    "e2e:ci": "start-server-and-test ci:start-server http://localhost:4200 cypress:run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@ngrx/store": "^14.1.0",
    "css-reset-and-normalize": "^2.3.6",
    "primeicons": "^5.0.0",
    "primeng": "^14.0.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-eslint/eslint-plugin": "^14.0.2",
    "@angular-eslint/builder": "^14.0.2",
    "@angular-eslint/eslint-plugin-template": "^14.0.2",
    "@angular-eslint/schematics": "^14.0.2",
    "@angular-eslint/template-parser": "^14.0.2",
    "@angular-devkit/build-angular": "^14.1.2",
    "@angular/cli": "~14.1.2",
    "@angular/compiler-cli": "^14.1.0",
    "@types/jasmine": "~4.0.0",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier": "2.5.1",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^3.0.0",
    "typescript": "~4.7.2"
  },
  "lint-staged": {
    "*.{js,ts,html}": "eslint --cache --fix",
    "*.{css,scss}": "stylelint --fix",
    "*.{js,ts,css,md,scss,html}": "prettier --write"
  }
}
